<h1>List of Users</h1>
    <table border="2">
  <tr>
    <th>User Name</th>
    <th>User Type</th>
    <th>Email</th>
    <th>Address</th>
    <th>Date of Birth</th>
    <th>Phone Number</th>
    <th>Date of Joining</th>
  </tr>
<% @users.each do |user| %>
   <tr>
       <td><%= user.name %></td>
       <td><%= user.user_type %></td>
       <td><%= user.email %></td>
       <td><%= user.address %></td>
       <td><%= user.dob %></td>
       <td><%= user.phone_num %></td>
       <td><%= user.doj %></td>
       <td> <%= link_to "Show", user %></td>
      <td> <%= link_to "Edit", edit_user_path(user) %></td>
      <td> <%= link_to "Delete", user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
       <%#= link_to "Delete User", new_user_path %>
   </tr>
<% end %>
</table>
<%= link_to "New User", new_user_path %>